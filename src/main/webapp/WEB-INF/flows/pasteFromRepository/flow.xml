<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://www.springframework.org/schema/webflow 
		http://www.springframework.org/schema/webflow/spring-webflow.xsd">

	<var name="model" class="org.springframework.ui.ExtendedModelMap" />

	<input name="idRegime" required="true" type="int" />
	<input name="idt" required="true" type="int" />

	<on-start>
			<evaluate expression="jaxbService.onStart(idt,model)"
				result="flowScope.pasteFromRepositoryForm" />
	</on-start>
	
	<view-state id="pasteId" model="pasteFromRepositoryForm" view="pasteFromRepository/pasteId">
		<binder>
			<binding property="pasteId" />
		</binder>
	</view-state>

</flow>
