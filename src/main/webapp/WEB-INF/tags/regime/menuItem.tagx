<?xml version="1.0" encoding="UTF-8"?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
	xmlns:spring="http://www.springframework.org/tags">
	<jsp:directive.attribute name="id" required="true"
		description="id message command" />
	<jsp:directive.attribute name="classes" required="true"
		description="classes" />

	<div id="${id }">
		<spring:message code="${id }" />
	</div>
	<script type="text/javascript">menuItem("${id}", "${classes}", ${id});</script>

</jsp:root>